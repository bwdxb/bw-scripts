tzutil /s "Arabian Standard Time"
$a = (gwmi win32_operatingsystem).caption
IF ($a -eq "Microsoft Windows Server 2016 Standard")
{
slmgr.vbs -ipk "3Y6JV-WTN9Q-MG6DT-MY3P9-JXDD2"
}
ELSEIF ($a -eq "Microsoft Windows Server 2008 R2 Standard")
{
slmgr.vbs -ipk "HK88Q-QM4FQ-YDJKY-6Q4CF-6KTP9"
}
ELSE
{
slmgr.vbs -ipk "BWFFG-F8N2W-7TC4D-8764Q-QRQWW"
}
slmgr.vbs -ato  
net user /add buzinessware Dreams01 
net localgroup administrators buzinessware /add 
wmic useraccount where "Name='administrator'" set PasswordExpires=false
wmic useraccount where "Name='buzinessware'" set PasswordExpires=false
netsh advfirewall firewall add rule name="ICMP Allow incoming V4 echo request" protocol=icmpv4:8,any dir=in action=allow
netsh advfirewall firewall set rule group="remote desktop" new enable=Yes  
netsh advfirewall firewall add rule name="buzinessware rdp" dir=in action=allow protocol=TCP localport=49226
REG.exe Add "HKEY_LOCAL_MACHINE\Software\Microsoft\ServerManager" /V DoNotOpenServerManagerAtLogon /t REG_DWORD /D 0x1 /F
REG.EXE DELETE "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Terminal Server" /v fDenyTSConnections /f
REG.EXE ADD "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Terminal Server" /v fDenyTSConnections /t REG_DWORD /d 0 /f 
REG.EXE DELETE "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Terminal Server\WinStations\RDP-Tcp" /v PortNumber /f 
REG.EXE ADD "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Terminal Server\WinStations\RDP-Tcp" /v PortNumber /d "49226" /t REG_DWORD /f
REG.EXE DELETE "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Active Setup\Installed Components\{A509B1A7-37EF-4b3f-8CFC-4F3A74704073}" /v IsInstalled /f 
REG.EXE ADD "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Active Setup\Installed Components\{A509B1A7-37EF-4b3f-8CFC-4F3A74704073}" /v IsInstalled /d "0" /t REG_DWORD /f
REG.EXE DELETE "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Active Setup\Installed Components\{A509B1A8-37EF-4b3f-8CFC-4F3A74704073}" /v IsInstalled /f 
REG.EXE ADD "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Active Setup\Installed Components\{A509B1A8-37EF-4b3f-8CFC-4F3A74704073}" /v IsInstalled /d "0" /t REG_DWORD /f
